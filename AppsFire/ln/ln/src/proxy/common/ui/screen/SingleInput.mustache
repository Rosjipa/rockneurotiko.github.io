<!--MAIN-FRAME-->
<div class="_single_input LyMain" role="main">

    <section>
        <dl class="MdMN14List">
            <dt>{{inputTitle}}<span class="MdTxtCount"><span class="_current_length"></span>/<span class="_max_length"></span></span></dt>
            <dd><ul class="MdMN15List">
                <li><div class="mdMN15Tbl01"><div class="mdMN15Cell mdMN15Input ExPlaceholder"><input {{#lowercase}} style="text-transform: lowercase;" {{/lowercase}} type="text" class="_text_input" placeholder="" value="{{defaultMessage}}"></div></div></li>

                <!--/MdMN15List--></ul>

            </dd>
            <!--/MdMN14List--></dl>
    </section>

    <div class="MdMN09BtnSet mdMN09Col"><ul class="mdMN09Ul">
        <li>
            <a class="MdBtn01 _confirm_btn ExDisabled" data-pressed-class="ExTap" data-disabled-class="ExDisabled" href="#">{{buttonLabel}}</a>
            <a class="MdBtn01 _register_btn ExDisabled MdNonDisp" data-pressed
            -class="ExTap" data-disabled-class="ExDisabled" href="#">{{buttonLabel}}</a>
        </li>
    </ul><!--/MdMN09BtnSet--></div>

    <p class="MdTxt01 _warn_message">{{warnMessage}}</p>


    <!--/LyMain--></div>
<!--/MAIN-FRAME-->